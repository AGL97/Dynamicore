{
  "expo": {
    "name": "Dynamicore",
    "slug": "Dynamicore",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/presentation/assets/images/icono-dynamic.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "permissions": [
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.INTERNET"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"
        }
      },
      "package": "com.agl97.Dynamicore"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/presentation/assets/images/splash-dinamycore.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permite a $(PRODUCT_NAME) usar tu localización para mostrar tu ubicación en el mapa."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
      
    },
    "owner": "agl97"
  }
}
